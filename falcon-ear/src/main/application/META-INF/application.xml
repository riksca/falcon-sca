<?xml version="1.0" encoding="UTF-8"?>
<application
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/application_5.xsd"
    version="5">

    <description>Card Marshal Application</description>
    <display-name>FALCON</display-name>

    <!-- Default Module -->
    <module>
        <web>
            <web-uri>falcon-ui-2.0</web-uri>
            <context-root>falcon-ui</context-root>
        </web>
    </module>
    <!-- Other Modules -->
    <module>
        <web>
            <web-uri>adminb-2.0</web-uri>
            <context-root>adminb</context-root>
        </web>
    </module>
    <module>
        <web>
            <web-uri>api-2.0</web-uri>
            <context-root>api</context-root>
        </web>
    </module>
</application>